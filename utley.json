{
    "css": [{
        "src": "resources/css",
        "output": "build/mobile.css",
        "version": "1.0.0",
        "exclude": [
            "mobile/home.css",
            "desktop"
        ]
    }, {
        "src": "resources/css",
        "output": "build/desktop.css",
        "version": "1.0.0",
        "exclude": [
            "desktop/home.css",
            "mobile"
        ]
    }],

    "js": [{
        "src": "/usr/local/www/PeteJS/src",
        "output": "build/pete.js",
        "version": "1.0.0",
        "dependencies": [
            "Pete.prototype.js",
            "Pete.js",
            "Pete.Element.js",
            "Pete.Composite.js",
            "Pete.Observer.js"
        ]
    }, {
        "src": "js",
        "output": "build/main.js",
        "version": "1.0.0"
    }]
}

